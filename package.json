    "scripts": {
        "start": "concurrently --raw \" ionic serve -lc\" \"npm run _server:start\" ",
        "build": "concurrently \"npm run _server:build\" \"npm run _client:build\" ",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start:app": "concurrently \"npm run server_build\" ",
        "debug:win": "concurrently --raw \"npm run _client:start\" \"npm run _server:start\" ",
        "debug:host": "concurrently \" ionic serve -lc\" \"npm run _server:start\" ",
        "_client:start": "ionic serve -lc --no-progress",
        "_client:build": "tsc -p ./client",
        "_server:start": "tsc-watch --project ./server/tsconfig.json --onSuccess \"node --inspect \"./dist/server/index.js\"\"",
        "_server:build": "tsc -p ./server",
        "_server:swagger": "npm run swagger-gen && npm run routes-gen && tsc -p ./server && tsc && npm run copy-swagger-ui && concurrently \"tsc -w --p ./server\" \"nodemon --inspect dist/server/start.js\" ",
        "swagger-gen": "tsoa swagger",
        "routes-gen": "tsoa routes",
        "copy-swagger-ui": "ncp ./server/swagger-ui ./dist/server/swagger-ui"
    },
